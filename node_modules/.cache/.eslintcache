(false)\n    const [valRules, setValRules] = useState(validations)\n    const valid = useValidation(value, valRules)\n\n    const onChange = (e, onInput) => {\n        if (e?.target?.value) {\n            setValue(e.target.value)\n            onInput(e.target.value);\n        } else {\n            setValue(e)\n            onInput(e);\n        }\n    }\n\n    const onBlur = () => {\n        setDirty(true)\n    }\n\n    useEffect(() => {\n        setValRules(validations)\n    }, [validations])\n\n    useEffect(() => {\n        setValue(initialValue)\n    }, [initialValue])\n\n    return {\n        value,\n        onChange,\n        onBlur,\n        isDirty,\n        ...valid\n    }\n}\n","/Users/sodii/Documents/hotels/src/components/layout/AppCheckbox/AppCheckbox.jsx",[],[],"/Users/sodii/Documents/hotels/src/components/layout/AppPhoneInput/AppPhoneInput.jsx",[],[],"/Users/sodii/Documents/hotels/src/components/layout/AppButton/AppButton.jsx",[],[],{"ruleId":"75","severity":1,"message":"76","line":1,"column":17,"nodeType":"77","messageId":"78","endLine":1,"endColumn":25},{"ruleId":"79","severity":1,"message":"80","line":115,"column":6,"nodeType":"81","endLine":115,"endColumn":8,"suggestions":"82"},{"ruleId":"79","severity":1,"message":"83","line":2,"column":58,"nodeType":"81","endLine":2,"endColumn":60,"suggestions":"84"},{"ruleId":"79","severity":1,"message":"85","line":27,"column":8,"nodeType":"81","endLine":27,"endColumn":15,"suggestions":"86"},"no-unused-vars","'useState' is defined but never used.","Identifier","unusedVar","react-hooks/exhaustive-deps","React Hook useEffect has missing dependencies: 'setAdults', 'setChildren', 'setInsurance', 'setKids', and 'setNights'. Either include them or remove the dependency array. If 'setAdults' changes too often, find the parent component that defines it and wrap that definition in useCallback.","ArrayExpression",["87"],"React Hook useEffect has a missing dependency: 'Func'. Either include it or remove the dependency array.